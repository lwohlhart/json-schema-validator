            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "address": {
                    "$ref": "#/definitions/Address"
                },
                "phone": {
                    "type": "string"
                },
                "website": {
                    "type": "string"
                },
                "company": {
                    "$ref": "#/definitions/Company"
                }
            },
            "required": [
                "address",
                "company",
                "email",
                "id",
                "name",
                "phone",
                "username",
                "website"
            ],
            "title": "User"
        },
        "Address": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
